html
    head
        title Sistema JMJ
        meta(charset="utf-8")
        meta(name="viewport", content="width=device-width, initial-scale=1, user-scalable=no")
        link(rel="stylesheet", href="/assets/css/main.css")
        link(rel="stylesheet", href="/assets/icomoon/style.css")
        //if lte IE 9
            link(rel="stylesheet", href="/assets/css/ie9.css")
        noscript
            link(rel="stylesheet", href="/assets/css/noscript.css")
    body
        // Wrapper
        #wrapper
            include ../usuario.pug  
            br
            br

            #main           
                article.active
                    include ../menu.pug   
                    br
                    br
                    
                    section
                        h3.major DADOS DA JMJ
                        form#excluir_jmj
                            .field.half.first
                                label(for="pais") País
                                input(type="text", name="pais", value=pais, disabled=1)
                            .field.half
                                label(for="cidade") Cidade
                                input(type="text", name="cidade", value=cidade,  disabled=1)                            
                            .field.half.first
                                label(for="ano") Ano
                                input(type="text", name="ano", value=ano,  disabled=1)
                            .field.half
                                label(for="periodo") Período
                                input(type="text", name="periodo", value=periodo,  disabled=1)
                            .field
                                label(for="lema") Lema
                                input(type="text", name="lema", value=lema,  disabled=1)
                            .field
                                label(for="nome") Deseja realmente excluir?                           
                                input#demo-priority-low(type="radio", name="demo-priority", )
                                label(for="demo-priority-low") Sim
                                input#demo-priority-high(type="radio", name="demo-priority", checked="")
                                label(for="demo-priority-high") Não
                            ul.actions(style="text-align: right")
                                li
                                    a.button.special(href="/jmj") Voltar 
                                li
                                    input#excluir(type="submit", value="Excluir", disabled=1, style="background: #FE514E; color: #ffffff")
                                
            // Footer
            footer#footer
